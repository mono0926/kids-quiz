  "version": "0.2.0",
  "configurations": [
    {
      "name": "kidsquiz_flutter",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port=57220"
      ]
    },
    {
      "name": "kidsquiz_flutter (tesla)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--web-port=57220",
        "--dart-define=TESLA=true"
      ]
    },
    {
      "name": "kidsquiz_flutter (tesla, release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "args": [
        "--web-port=57220",
        "--dart-define=TESLA=true"
      ]
    },
    {
      "name": "kidsquiz_flutter (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "kidsquiz_flutter (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
