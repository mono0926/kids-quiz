rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // match /users/{userId=**} {
    //   allow read, write: if request.auth.uid == userId;
    // }
    match /{document=**} {
      allow read, write: if request.time < timestamp.date(2019, 12, 31);
    }
  }
}
